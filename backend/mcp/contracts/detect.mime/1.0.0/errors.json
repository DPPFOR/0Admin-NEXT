{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "urn:0admin:mcp:detect.mime:1.0.0:errors",
  "title": "detect.mime errors 1.0.0",
  "type": "object",
  "additionalProperties": false,
  "$defs": {
    "uuid": {"type": "string", "format": "uuid", "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"},
    "base64": {"type": "string", "pattern": "^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$"},
    "rfc3339": {"type": "string", "format": "date-time", "pattern": ".*(Z|[+-]\\d{2}:\\d{2})$"},
    "iso_duration": {"type": "string", "pattern": "^(P(?=.+)(?:\\d+W|\\d+D(?:T(?:\\d+H)?(?:\\d+M)?(?:\\d+S)?)?|T(?:\\d+H)?(?:\\d+M)?(?:\\d+S)?))$"}
  },
  "properties": {"code": {"type": "string", "enum": ["VALIDATION", "NOT_FOUND", "UPSTREAM", "POLICY_DENIED", "RETRYABLE_IO"]}, "message": {"type": "string"}},
  "required": ["code"]
}
